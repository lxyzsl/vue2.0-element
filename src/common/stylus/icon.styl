@font-face
  font-family: 'sell-icon'
  // 这个url是相对引入icon.styl的goods.vue组件的路径
  src: url('../../common/fonts/sell-icon.eot?2qevqt')
  src: url('../../common/fonts/sell-icon.eot') format('embedded-opentype'),
          url('../../common/fonts/sell-icon.ttf?2qevqt') format('truetype'),
          url('../../common/fonts/sell-icon.woff?2qevqt') format('woff'),
          url('../../common/fonts/sell-icon.svg?2qevqt#sell-icon') format('svg')
  font-weight: normal
  font-style: normal

[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'sell-icon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

